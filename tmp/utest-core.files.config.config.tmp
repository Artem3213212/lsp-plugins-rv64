# Visibility of mount studs in the UI [boolean]: true/false
mount_stud = true

# Last version of the product installed [pathname]
version = "1.1.28"

# Dialog path for selecting sample files [pathname]
dlg_sample_path = "/path/sample"

# Dialog path for selecting impulse response files [pathname]
dlg_ir_path = "/path/ir"

# Dialog path for saving/loading configuration files [pathname]
dlg_config_path = "/path/to/config"

# Dialog default path for other files [pathname]
dlg_default_path = "/path/by/default"

# Some integer KVT parameter 1
/kvt/some/param1 = i32:12345

# Some float KVT parameter 2
/kvt/some/param2 = f32:"12345.678"

# Some text KVT parameter 3
/kvt/some/param3 = str:"Test string"

/kvt/some/param4 = blob:"application/binary:9:123412341234"

